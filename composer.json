{
    "name": "pomm-project/pomm-symfony-bridge",
    "type": "library",
    "description":  "Pomm2 shared elements between Silex and Symfony.",
    "keywords": ["silex", "symfony","pomm", "debug", "profiler"],
    "homepage": "http://www.pomm-project.org",
    "license":  "MIT",
    "authors":  [ { "name": "Grégoire HUBERT", "email": "hubert.greg@gmail.com" }, { "name": "Jérôme MACIAS", "email": "jerome.macias@gmail.com" } ],
    "require": {
        "pomm-project/foundation": "~2.0",
        "symfony/http-foundation": "~2.5|~3.0",
        "symfony/http-kernel": "~2.5|~3.0",
        "symfony/serializer": "~2.5|~3.0",
        "symfony/property-info": "~2.8|~3.0",
        "symfony/validator": "~2.8|~3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.7"
    },
    "suggest": {
        "pomm-project/model-manager": "~2.0",
        "pomm-project/cli": "~2.0",
        "silex/silex": "~1.0",
        "symfony/symfony": "~2.5|~3.0"
    },
    "autoload": {
        "psr-4": {
            "PommProject\\SymfonyBridge\\": "sources/lib"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
