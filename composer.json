{
    "name": "conserto/pomm-symfony-bridge",
    "type": "library",
    "description":  "Pomm profiler for Symfony.",
    "keywords": ["symfony","pomm", "debug", "profiler"],
    "homepage": "http://www.pomm-project.org",
    "license":  "MIT",
    "authors":  [ { "name": "Grégoire HUBERT", "email": "hubert.greg@gmail.com" }, { "name": "Jérôme MACIAS", "email": "jerome.macias@gmail.com" } ],
    "require": {
        "php": ">=8.1",
        "conserto/pomm-foundation": "^3.0",
        "conserto/pomm-model-manager": "^3.0",
        "symfony/http-foundation": "~5.0|~6.0",
        "symfony/http-kernel": "~5.0|~6.0",
        "symfony/serializer": "~5.0|~6.0",
        "symfony/property-info": "~5.0|~6.0",
        "symfony/stopwatch": "~5.0|~6.0",
        "twig/twig": "~3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.7",
        "phpstan/phpstan": "^1.8"
    },
    "suggest": {
        "conserto/pomm-cli": "~3.0",
        "symfony/symfony": "~5.0|~6.0"
    },
    "autoload": {
        "psr-4": {
            "PommProject\\SymfonyBridge\\": "sources/lib"
        }
    },
    "minimum-stability": "stable"
}
